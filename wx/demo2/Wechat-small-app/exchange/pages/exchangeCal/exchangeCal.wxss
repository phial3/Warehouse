page{
    background-color:#D6D6D6;
}

.container{
    background-color:#D6D6D6;
    height:100%;
}

.fl{
    float:left;
}

.fr{
    float:right;
}

.currencyTitle{
    line-height: 26px;
    font-size: 12px;
    color: #666;
    text-indent: 15px;
}

.item{
    width:100%;
    height: 88px;
    border-bottom: 1px solid #d4d4d4;
    border-top: 1px solid #d4d4d4;
    background: #fff;
}

.itemLeft{
    background: url(../../images/arrow.png) right no-repeat;
    width: 35%;
    border-right: 1px #e5e5e5 solid;
    margin-top: 15px;
    height: 40px;
}

.flag{
    width:60px;
    height:40px;
    margin-left: 20px;
}

.itemRight{
    text-align: right;
    border: 0;
    font-size: 16px;
    color: #333;
    margin-top: 35px;
    margin-right: 10%;
    width: 54%;
}

.clear{
    clear: both;
}

.currencyDes{
    color: #999;
    margin-left: 12px;
    margin-top: 5px;
    font-size: 16px;
}

.currencyDesText{
    margin: 0 1%;
}

.calResult{
    font-size: 12px;
    color: #999;
    margin-left: 15px;
    line-height: 30px;
}

.calNum{
    color: #333;
}

.dateArea{
    line-height: 42px;
    font-size: 16px;
    color: #333;
    border-bottom: 1px solid #d4d4d4;
    border-top: 1px solid #d4d4d4;
    background: url(../../images/arrow.png) right no-repeat #fff;
    text-indent: 4.3%;
    margin-top:20px;
}

.dateTitle{
    padding-right:20px;
}

.errorToast{
    text-align: center;
    color: #fff;
    padding: 10px;
    font-size: 12px;
    color: #fff;
}