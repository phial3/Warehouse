/**app.wxss**/
.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 200rpx 0;
  box-sizing: border-box;
} 
.center{
    text-align: center;
    font-size: 20px;
    padding: 15px 0 

}
.content{
    line-height: 32px;
    font-size: 18px;
}

.section,.btn-area{
  margin: 15px 5px;
}
.del{
  color: red;
  margin-bottom: 15px;
  margin-top: 5px;
}

.title{
  margin: 5px 0;
  font-size: 20px;
}
.section__title{
  
  margin-bottom: 15px;
  font-size: 24px;

}
.btn-area button{
  margin-bottom: 15px;
}

input{
  margin-top: 15px;
  border: 1px solid #ddd;
  padding: 8px 5px;
  background-color: #ddd;
}

form{
  background-color: #ddd;
}

.log-list image,image{

  width: 100px;
  height: 100px;
}
