.container {
  background: rgba(255,255,255,0.8);
}
scroll-view {
  width: 100%;
  height: 100%;
}
.received_events {
  border-bottom: 1rpx solid #E2E2E2;
  padding: 20rpx;
}
.actor_avatar {
  width: 80rpx;
  height: 80rpx;
  margin-right: 20rpx;
}
.actor_name {
  font-size: 25rpx;
  color: #4078c0;
}
.created_at {
  margin-top: 5rpx;
  font-size: 20rpx;
  color: #9A9A9A;
}
.action_type {
  font-size: 25rpx;
  font-weight: bold;
  margin-right: 20rpx;
  color: #333333;
}
.action_repo {
  font-size: 25rpx;
  color: #4078c0;
}
