page{
	background-color: #f8f8f8;
}

.list-group{
	margin: 20px 0;
	border-width: 0 0 1rpx;
	border-style: solid;
	border-color: #b2b2b2;
}

.list{
	background-color: #ffffff;
	border-width: 1rpx 0 0;
	border-style: solid;
	border-color: #b2b2b2;
	height: 20px;
	padding: 10px 15px;
	position: relative;
}

text{
	position: absolute;
	font-size: 17pt;
}

.icon{
	height: 20px;
	width: 20px;
	margin-right:15px;
}

.arrow{
	height: 20px;
	width: 20px;
	position: absolute;
	right: 10px;
}

.clear{
	height: 44px;
	margin: 0 15px;
}